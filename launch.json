{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "envFile": "${workspaceFolder}/.env",   
            "console": "integratedTerminal",
            "env": {
                "AZURE_DEVOPS_PAT": "sua_personal_access_token",
                "AZURE_DEVOPS_ORG": "sua_organização",
                "AZURE_DEVOPS_PROJECT": "seu_projeto"
            }
        }
    ]
}
